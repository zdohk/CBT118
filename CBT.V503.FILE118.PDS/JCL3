//SORTIN   DD  DSN=&&SORT,DISP=(NEW,PASS),UNIT=SYSDA,
//             DCB=(RECFM=FB,LRECL=15,BLKSIZE=4095),
//             SPACE=(CYL,(55,30))
//SORTOUT  DD  DSN=&&SORT,DISP=(SHR,PASS),VOL=REF=*.SORTIN
//TITLES   DD  DISP=SHR,DSN=WCBT.V436.FILE118.PDS(TITLES)
//PRINTER  DD  SYSOUT=*
//SMPCOUT  DD DISP=SHR,DSN=WSBG.PTFPDS(TESTSEQO)    <== OPTIONAL DDNAME
//*                    <== FB-80, PRODUCES OUTPUT LIKE SMPASUPD PROGRAM
//PDSATOUT DD DISP=SHR,DSN=WSBG.PTFPDS2(ESO0105A)   <== OPTIONAL DDNAME
//*                    <== FB-80, PRODUCES ATTRIB INPUT TO PDS 8.5
